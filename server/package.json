{
  "name": "typescript-solid-analyzer",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.2",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/platform-fastify": "^7.4.2",
    "@nestjs/platform-ws": "^7.4.2",
    "@nestjs/websockets": "^7.4.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "connect-mongodb-session": "^2.4.1",
    "extract-zip": "^2.0.1",
    "fastify": "^3.2.1",
    "fastify-cookie": "^4.0.2",
    "fastify-csrf": "^1.0.4",
    "fastify-helmet": "^5.0.1",
    "fastify-metrics": "^6.0.2",
    "fastify-reply-from": "^3.1.4",
    "fastify-session": "^5.0.0",
    "mongoose": "^5.10.0",
    "node-fetch": "^2.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "swagger-stats": "^0.95.17",
    "tmp-promise": "^3.0.2",
    "workerpool": "^6.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.4.2",
    "@types/connect-mongodb-session": "0.0.3",
    "@types/jest": "^26.0.10",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/rimraf": "^3.0.0",
    "@types/supertest": "^2.0.10",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  }
}
